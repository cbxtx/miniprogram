<!--pages/register/register.wxml-->
<!-- <text>pages/register/register.wxml</text> -->
<view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">姓名</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder-class="weui-input__placeholder" placeholder="填写姓名" bindchange="changeName" />
        </view>
    </view>
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">手机号</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder-class="weui-input__placeholder" placeholder="填写手机号" bindchange="changePhone" />
        </view>
    </view>
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">学校</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder-class="weui-input__placeholder" placeholder="填写学校" bindchange="changeSchool" />
        </view>
    </view>
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">学号</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder-class="weui-input__placeholder" placeholder="填写学号" bindchange="changeStudentId" />
        </view>
    </view>
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">入学年份</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder-class="weui-input__placeholder" placeholder="填写入学年份如2020" bindchange="changeEnrollmentYear" />
        </view>
    </view>
    <view class="page__bd page__bd_spacing submit">
        <button class="weui-btn" type="primary" bindtap="bindSubmit">提交</button>
    </view>
</view>